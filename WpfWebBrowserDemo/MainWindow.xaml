<Window x:Class="WpfWebBrowserDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfWebBrowserDemo"
        mc:Ignorable="d"
        Title="WebBrowser">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="584*"/>
            <ColumnDefinition Width="63*"/>
            <ColumnDefinition Width="55*"/>
            <ColumnDefinition Width="61*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="27*"/>
            <RowDefinition Height="296*"/>
        </Grid.RowDefinitions>
        <TextBox Grid.Row="0" Grid.Column="0" Name="textBox" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
        <Button Grid.Row="0" Grid.Column="1" Name="button" Grid.ColumnSpan="1" Content="GO" Click="Button_Click" />
        <Button Grid.Row="0" Grid.Column="2" Name="buttonPrint" Content="Print" Click="ButtonPrint_Click" />
        <WebBrowser Name="webBrowser" Grid.ColumnSpan="4" Grid.RowSpan="2" Margin="0,42,0,-0.5"/>
        <Button Grid.Row="0" Grid.Column="3" Name="buttonJS" Content="JS" Click="ButtonJS_Click"/>
    </Grid>
</Window>
